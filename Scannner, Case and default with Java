	    System.out.println("How old are you?");
	    Scanner yourAge = new Scanner(System.in);  

	    String myAge = yourAge.nextLine();  // Read user input
	    System.out.println(" Your Age is: " + myAge);  // Output user input
	  }



 Scanner myObj = new Scanner(System.in);

		    System.out.println("Enter name, age and salary:");

		    // String input
		    String name = myObj.nextLine();

		    // Numerical input
		    int age = myObj.nextInt();
		    double salary = myObj.nextDouble();

		    // Output input by user
		    System.out.println("Name: " + name);
		    System.out.println("Age: " + age);
		    System.out.println("Salary: " + salary);







String coffeeType;
Scanner coffeeNumber = new Scanner (System.in);
System.out.println("What type of Coffee would you like?");

int passcode = coffeeNumber.nextInt();
	
	switch (passcode){
		case 555: coffeeType = "Espresso";
				break;
		case 629: coffeeType = "Vanilla latte";
		break;
		case 312: coffeeType = "Drip coffee";
		default: coffeeType = "Unknown";
		break;
	} 
	System.out.println(coffeeType);


}}
		  
